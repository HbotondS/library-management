// get books which were published between 2000 and 2005
use('library-management')

db.Book.aggregate([
    {
        $match: {
            publishedDate: {
                $gte: new Date("2000-01-01"),
                $lt: new Date("2006-01-01")
            }
        }
    },
    {
        $project: {
            title: 1,
            publishedDate: 1
        }
    }
])
